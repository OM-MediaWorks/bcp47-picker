{
  "name": "bcp47-picker",
  "version": "1.0.0",
  "description": "A CustomElement following: https://www.w3.org/International/questions/qa-choosing-language-tags",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbeeke/bcp47-picker.git"
  },
  "author": "Daniel Beeke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielbeeke/bcp47-picker/issues"
  },
  "homepage": "https://github.com/danielbeeke/bcp47-picker#readme",
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "postinstall": "node --experimental-fetch scripts/fetch-data.js",
    "build": "microbundle",
    "dev": "microbundle watch --no-sourcemap -f cjs",
    "demo": "parcel demo/index.html",
    "build-demo": "rm -rf docs/ && parcel build demo/index.html --dist-dir docs --target demo && echo \"bcp47.danielbeeke.nl\" > docs/CNAME"
  },
  "targets": {
    "demo": {
      "engines": {
        "browsers": "> .5% or last 2 versions"
      }
    }
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.5.0",
    "@parcel/transformer-sass": "^2.5.0",
    "cors-anywhere": "^0.4.4",
    "microbundle": "^0.15.0",
    "parcel": "^2.4.1"
  },
  "dependencies": {
    "bcp-47": "^2.1.0",
    "bcp-47-normalize": "^2.1.0",
    "bootstrap-icons": "^1.8.1",
    "iso-15924": "^3.1.0",
    "iso-3166": "^4.0.1",
    "iso-639-3": "^3.0.1",
    "memoize-getters": "^1.1.0",
    "sorted-btree": "^1.8.0",
    "trie-search": "^1.3.6",
    "uhtml": "^3.0.0",
    "un-m49": "^2.0.1"
  }
}
